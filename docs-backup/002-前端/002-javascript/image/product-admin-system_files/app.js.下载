dex = 15 + i\n      } else {\n        index = i\n      }\n      if (localStorage.getItem('postSelectionItem')) {\n        localStorage.removeItem('postSelectionItem')\n      }\n      if (localStorage.getItem('recievePostSelectionItem')) {\n        localStorage.removeItem('recievePostSelectionItem')\n      }\n      let copyTagsLists = this.tagsList.slice()\n      // 获取即将关闭的页面数据\n      const delItem = copyTagsLists.splice(index, 1)[0]\n      // 截取带有参数的路由path（提供给删除缓存用）\n      const delPath = delItem.path.split('?')[0]\n      const item = copyTagsLists[index] ? copyTagsLists[index] : copyTagsLists[index - 1]\n      if (item) {\n        delItem.path === this.$route.fullPath && this.$router.push(item.path)\n      } else {\n        this.$router.push(this.jumpPath)\n      }\n      if (this.jumpPath != '/') {\n        this.$router.push(this.jumpPath)\n      }\n      // vuex判断当前页面是否弹出未保存提示\n      if (!this.$store.state.canClosePageTag) {\n        this.unwatch = this.$watch('$store.state.canClosePageTag', (newVal) => {\n          // 点击了确定可以离开页面\n          if (newVal) {\n            this.tagsList.splice(index, 1)[0]\n            delete this.getCacheArr.cache[delPath]\n            bus.$emit('tags', this.tagsList)\n            sessionStorage.setItem('tageData', JSON.stringify(this.tagsList))\n            this.unwatch()\n            this.flattenArrays(this.tagsList)\n          }\n        })\n        this.$store.commit('setCanClosePageWatch', this.unwatch)\n      } else {\n        this.tagsList.splice(index, 1)[0]\n        delete this.getCacheArr.cache[delPath]\n        bus.$emit('tags', this.tagsList)\n        sessionStorage.setItem('tageData', JSON.stringify(this.tagsList))\n        this.jumpPath = '/'\n        this.flattenArrays(this.tagsList)\n      }\n    },\n    // 清除缓存\n    deleteCacheData() {},\n    // 关闭全部标签\n    closeAll() {\n      // vuex判断当前页面是否弹出未保存提示\n      if (!this.$store.state.canClosePageTag) {\n        this.unwatch = this.$watch('$store.state.canClosePageTag', (newVal) => {\n          // 点击了确定可以离开页面\n          if (newVal) {\n            this.tagsList = []\n            sessionStorage.setItem('tageData', JSON.stringify(this.tagsList))\n            this.unwatch()\n          }\n        })\n        this.$store.commit('setCanClosePageWatch', this.unwatch)\n      } else {\n        this.tagsList = []\n        sessionStorage.setItem('tageData', JSON.stringify(this.tagsList))\n      }\n      sessionStorage.removeItem('tageData') // 退出后删除所有导航页签\n      this.$router.push('/')\n    },\n    // 关闭其他标签\n    closeOther() {\n      const curItem = this.tagsList.filter((item) => {\n        return item.path === this.$route.fullPath\n      })\n      this.tagsList = curItem\n      sessionStorage.removeItem('tageData') // 先删除所有导航页签\n      sessionStorage.setItem('tageData', JSON.stringify(this.tagsList)) //在把当前标签存进去\n      this.flattenArrays(this.tagsList)\n    },\n    // 设置标签\n    setTags(route) {\n      this.tagsList = !!JSON.parse(sessionStorage.getItem('tageData')) ? JSON.parse(sessionStorage.getItem('tageData')) : []\n      if (route.query && !!route.query.check) {\n        if (route.query.check == 'check' && route.meta.title.indexOf('查看') == -1) {\n          route.meta.title = route.meta.title.replace(/新增/, '查看')\n          route.meta.title = route.meta.title.replace(/编辑/, '查看')\n        }\n        if (route.query.check == '1' || route.query.check == '2') {\n          route.meta.title = route.meta.title.replace(/新增/, '编辑')\n          route.meta.title = route.meta.title.replace(/查看/, '编辑')\n        }\n      }\n      if (!route.query.check && !route.query.id) {\n        route.meta.title = route.meta.title.replace(/查看/, '新增')\n        route.meta.title = route.meta.title.replace(/编辑/, '新增')\n      }\n      const isExist = this.tagsList.some((item) => {\n        return item.path === route.fullPath\n      })\n      if (!isExist) {\n        if (this.tagsList.length >= 30) {\n          let obj = this.tagsList[0]\n          delete this.getCacheArr.cache[obj.path]\n          this.tagsList.shift()\n        }\n        this.tagsList.forEach((e, i) => {\n          if (e.title === route.meta.title) {\n            this.tagsList.splice(i, 1)\n          }\n        })\n        this.tagsList.push({\n          title: route.meta.title,\n          path: route.fullPath,\n          name: route.matched[1].components.default.name,\n        })\n      }\n      bus.$emit('tags', this.tagsList)\n      bus.$emit('targetMenu', route.meta.module)\n      sessionStorage.setItem('tageData', JSON.stringify(this.tagsList))\n      this.flattenArrays(this.tagsList)\n    },\n    handleTags(command) {\n      if (command === 'other') {\n        this.closeOther()\n      } else if (command === 'all') {\n        this.closeAll()\n      } else if (command === 'target') {\n        const curItem = this.tagsList.filter((item) => {\n          return item.path === this.$route.fullPath\n        })\n        let targetIndex = null\n        this.tagsList.forEach((e, i) => {\n          if (e.path === curItem[0].path) {\n            targetIndex = i\n          }\n        })\n        this.closeTags(targetIndex)\n      }\n    },\n    // 将tagsList数组转化为二维数组，用于点击轮播，如果打开超过15个标签，就把第16个页签存放在第二个二维数组中，每个二维数组有15个页签\n    flattenArrays(tagsList) {\n      this.$nextTick(() => {\n        let index = 0\n        this.totalArrays = []\n        let num\n        let textArr = [] // 用于存放标签中的title文字，然后转为字符转，一个字符串是14px，计算它的长度\n        tagsList.forEach((e) => {\n          textArr.push(e.title)\n        })\n        // 计算比例值\n        let tagsWidth = (this.$refs.tags.clientWidth - 110) / (textArr.join().length * 14 + tagsList.length * 17)\n\n        if (tagsWidth > 1) {\n          // 还没超过标签盒子尺寸\n          num = tagsList.length\n        } else if (tagsWidth <= 1 && tagsWidth > 0.9) {\n          // 大体为刚刚超过，这时候存下来，后面凡是比例值小于0.9的都用这个\n          num = tagsList.length\n          sessionStorage.setItem('tagsNum', num)\n        } else {\n          num = sessionStorage.getItem('tagsNum') - 1\n        }\n\n        console.log('tagsWidth', this.$refs.tags.clientWidth - 110, tagsWidth, num)\n        while (index < tagsList.length) {\n          this.totalArrays.push(tagsList.slice(index, (index += num)))\n        }\n      })\n    },\n  },\n  computed: {\n    showTags() {\n      return this.tagsList.length > 0\n    },\n  },\n  watch: {\n    $route(newValue, oldValue) {\n      if (!!newValue.query && !!newValue.query.id && !!newValue.query.check) {\n        if (newValue.meta.title.indexOf('新增') > -1) {\n          newValue.meta.title = newValue.meta.title.replace(/新增/, '编辑')\n        }\n        if (newValue.query.check == 'check') {\n          newValue.meta.title = newValue.meta.title.replace(/编辑/, '查看')\n        } else {\n          newValue.meta.title = newValue.meta.title.replace(/查看/, '编辑')\n          newValue.meta.title = newValue.meta.title.replace(/新增/, '编辑')\n        }\n      } else {\n        if (!!newValue.query.edit) {\n        } else {\n          newValue.meta.title = newValue.meta.title.replace(/编辑/, '新增')\n        }\n      }\n      this.setTags(newValue)\n    },\n    immediate: true,\n    deep: true,\n  },\n  created() {\n    this.setTags(this.$route)\n    let self = this\n    // 监听关闭当前页面的标签页\n    bus.$on('close_current_tags', () => {\n      for (let i = 0, len = this.tagsList.length; i < len; i++) {\n        const item = this.tagsList[i]\n        if (item.path === this.$route.fullPath) {\n          if (i < len - 1) {\n            this.$router.push(this.tagsList[i + 1].path)\n          } else if (i > 0) {\n            this.$router.push(this.tagsList[i - 1].path)\n          } else {\n            this.$router.push('/')\n          }\n          this.tagsList.splice(i, 1)\n          break\n        }\n      }\n      this.flattenArrays(this.tagsList)\n    })\n    bus.$on('tagsIndex', function (data) {\n      self.jumpPath = data.jumpPath\n      self.closeTags(data.index)\n    })\n    this.flattenArrays(this.tagsList)\n  },\n}\n</script>\n\n<style >\n.tags {\n  position: relative;\n  height: 30px;\n  overflow: hidden;\n  background: #fff;\n  padding-right: 120px;\n  border-bottom: 1px solid #f5f7fa;\n}\n\n.tags ul {\n  box-sizing: border-box;\n  width: 100%;\n  height: 100%;\n  padding-left: 1px;\n}\n\n.tags-li {\n  float: left;\n  margin: 3px 4px 2px 3px;\n  border-radius: 3px;\n  font-size: 12px;\n  overflow: hidden;\n  cursor: pointer;\n  height: 23px;\n  line-height: 23px;\n  border: 1px solid #e9eaec;\n  background: #fff;\n  padding: 0 5px 0 5px;\n  vertical-align: middle;\n  color: #666;\n  -webkit-transition: all 0.3s ease-in;\n  -moz-transition: all 0.3s ease-in;\n  transition: all 0.3s ease-in;\n}\n\n.tags-li:not(.active):hover {\n  background: #f8f8f8;\n}\n\n.tags-li.active {\n  color: #fff;\n}\n\n.tags-li-title {\n  float: left;\n  max-width: 80px;\n  overflow: hidden;\n  white-space: nowrap;\n  text-overflow: ellipsis;\n  margin-right: 5px;\n  color: #666;\n}\n\n.tags-li.active .tags-li-title {\n  color: #fff;\n}\n\n.tags-close-box {\n  position: absolute;\n  right: 0;\n  top: 0;\n  box-sizing: border-box;\n  padding-top: 1px;\n  text-align: center;\n  width: 110px;\n  height: 30px;\n  background: #fff;\n  box-shadow: -3px 0 15px 3px rgba(0, 0, 0, 0.1);\n  z-index: 10;\n}\n</style>\n<style lang=\"less\" scoped>\n/deep/.el-carousel__arrow {\n  width: 22px;\n  height: 22px;\n\n  /* right: 0px; */\n}\n/deep/.el-carousel__arrow--left {\n  left: 0px;\n}\n/deep/.el-carousel__arrow--right {\n  right: 0px;\n}\n/deep/.el-carousel__item,\n.el-carousel__mask {\n  left: 20px;\n}\n</style>\n"],"sourceRoot":""}]);

