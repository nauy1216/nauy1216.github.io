var mixin = {

  methods: {
    /**
     * 格式化时间
     * @param {string|number|object|Array} dateTime - 时间，可以是一个字符串、时间戳、表示时间的对象、Date对象或者******表示时间的数组
     * @param {string} [fmt] - 格式
     * @returns {string} 返回格式化后的日期时间，默认格式：2018年1月11日 15:00
     * @see [momentjs]{@tutorial http://momentjs.cn/}
     */
    formateTime: function formateTime(value) {
      if (!!value) {
        var t = new Date(value);
        var y = t.getFullYear(),
            m = t.getMonth() + 1 > 10 ? t.getMonth() + 1 : '0' + (t.getMonth() + 1),
            d = t.getDate() > 10 ? t.getDate() : '0' + t.getDate(),
            h = t.getHours() >= 10 ? t.getHours() : '0' + t.getHours(),
            mm = t.getMinutes() > 10 ? t.getMinutes() : '0' + t.getMinutes(),
            s = t.getSeconds() > 10 ? t.getSeconds() : '0' + t.getSeconds();
        return y + '-' + m + '-' + d + ' ' + h + ':' + mm + ':' + s;
      }
    },
    deleteCach: function deleteCach(obj) {
      if (obj.$vnode && obj.$vnode.data.keepAlive) {
        if (obj.$vnode.parent && obj.$vnode.parent.componentInstance && obj.$vnode.parent.componentInstance.cache) {
          if (obj.$vnode.componentOptions) {
            var key = obj.$vnode.key == null ? obj.$vnode.componentOptions.Ctor.cid + (obj.$vnode.componentOptions.tag ? '::' + obj.$vnode.componentOptions.tag : '') : obj.$vnode.key;
            var cache = obj.$vnode.parent.componentInstance.cache;
            var keys = obj.$vnode.parent.componentInstance.keys;
            if (cache[key]) {
              if (keys.length) {
                var index = keys.indexOf(key);
                if (index > -1) {
                  keys.splice(index, 1);
                }
              }
              delete cache[key];
              sessionStorage.removeItem('cacheData');
            }
          }
        }
      }
    },

    // 进入指定的路由缓存
    destroyedDeom: function destroyedDeom(paths) {
      console.log(3333333);
      var instance = void 0;
      return {
        beforeRouteEnter: function beforeRouteEnter(to, from, next) {
          console.log(444444);
          to.meta.keepAlive = true;
          next(function (vm) {
            instance = vm;
          });
        },
        beforeRouteLeave: function beforeRouteLeave(to, from, next) {
          console.log(555555);
          if (!(Array.isArray(paths) && paths.indexOf(to.path) > -1)) {
            instance && instance.$destroy();
            instance = null;
          }
          next();
        }
      };
    },

    // 新增、编辑页面时，清除已经存在的页面的缓存
    delCacheHandle: function delCacheHandle(val) {
      var _this = this;

      console.log('清除缓存', val);
      var tageData = JSON.parse(sessionStorage.getItem('tageData'));
      tageData.forEach(function (e, i) {
        // 截取掉有参数的路由
        var path = e.path.split('?')[0];
        if (path === val) {
          // 先清除即将跳转的页面的标签path
          tageData.splice(i, 1);
          // 关闭已经存在的标签
          _this.$tool.closePath([val]);
        }
      });
      // 更新标签列表
      sessionStorage.setItem('tageData', __WEBPACK_IMPORTED_MODULE_0_babel_runtime_core_js_json_stringify___default()(tageData));
    }
  }
};
/* harmony default export */ __webpack_exports__["a"] = (mixin);

