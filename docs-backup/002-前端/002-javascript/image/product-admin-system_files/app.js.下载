/* harmony default export */ __webpack_exports__["a"] = ({
  //日期格式  yyyy-MM-HH
  formatTime: function formatTime(_date) {
    if (!_date) {
      return "-";
    }
    var date = new Date(_date);
    var y = date.getFullYear();
    var m = date.getMonth() + 1;
    m = m < 10 ? '0' + m : m;
    var d = date.getDate();
    d = d < 10 ? '0' + d : d;
    return y + '-' + m + '-' + d;
  },

  //日期格式  yyyy-MM-HH hh:mm:ss
  formatTimeFull: function formatTimeFull(_date) {
    if (!_date) {
      return "-";
    }
    var date = new Date(_date);
    var y = date.getFullYear();
    var m = date.getMonth() + 1;
    m = m < 10 ? '0' + m : m;
    var d = date.getDate();
    d = d < 10 ? '0' + d : d;
    var h = date.getHours();
    h = h < 10 ? '0' + h : h;
    var mn = date.getMinutes();
    mn = mn < 10 ? '0' + mn : mn;
    var hm = date.getSeconds();
    hm = hm < 10 ? '0' + hm : hm;
    return y + '-' + m + '-' + d + ' ' + h + ':' + mn + ':' + hm;
  },

  //检查空值的请求参数过滤
  checkNoValue: function checkNoValue(params) {
    var newParam = {};
    var newKey = __WEBPACK_IMPORTED_MODULE_0_babel_runtime_core_js_object_keys___default()(params).sort();
    for (var i = 0; i < newKey.length; i++) {
      //遍历newkey数组
      var value = params[newKey[i]];
      if (value !== null && value !== undefined && value !== "") {
        newParam[newKey[i]] = value;
      }
    }
    return newParam;
  },

  //todo 复制A对象数据到B对象
  copyValue: function copyValue(param1, param2) {

    var newKey = __WEBPACK_IMPORTED_MODULE_0_babel_runtime_core_js_object_keys___default()(param1).sort();
    for (var i = 0; i < newKey.length; i++) {
      //遍历newkey数组
      var value = param1[newKey[i]];
      // if (value === null || value === undefined || value === "" ){
      if (value) {
        param2[newKey[i]] = value;
      }
    }
    return param2;
  },

  //todo 检查等级
  checkLevel: function checkLevel(value) {
    var level = '无';
    switch (value) {
      case 0:
        level = '无';
        break;
      case 1:
        level = '一级';
        break;
      case 2:
        level = '二级';
        break;
      case 3:
        level = '三级';
        break;
      case 4:
        level = '四级';
        break;
      case 5:
        level = '五级';
        break;
    }
    return level;
  },
  uuid: function uuid(len, radix) {
    var chars = '0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz'.split('');
    var uuid = [],
        i;
    radix = radix || chars.length;

    if (len) {
      // Compact form
      for (i = 0; i < len; i++) {
        uuid[i] = chars[0 | Math.random() * radix];
      }
    } else {
      // rfc4122, version 4 form
      var r;
      // rfc4122 requires these characters
      uuid[8] = uuid[13] = uuid[18] = uuid[23] = '-';
      uuid[14] = '4';

      // Fill in random data.  At i==19 set the high bits of clock sequence as
      // per rfc4122, sec. 4.1.5
      for (i = 0; i < 36; i++) {
        if (!uuid[i]) {
          r = 0 | Math.random() * 16;
          uuid[i] = chars[i == 19 ? r & 0x3 | 0x8 : r];
        }
      }
    }

    return uuid.join('');
  },
  dispatchEventLocStroage: function dispatchEventLocStroage() {
    var signSetItem = localStorage.setItem;
    localStorage.setItem = function (key, val) {
      var setEvent = new Event('setItemEventStorage');
      setEvent.key = key;
      setEvent.newValue = val;
      window.dispatchEvent(setEvent);
      signSetItem.apply(this, arguments);
    };
  },
  dispatchEventSesStroage: function dispatchEventSesStroage() {
    var signSetItem = sessionStorage.setItem;
    sessionStorage.setItem = function (key, val) {
      var setEvent = new Event('setItemEventSession');
      setEvent.key = key;
      setEvent.newValue = val;
      window.dispatchEvent(setEvent);
      signSetItem.apply(this, arguments);
    };
  },

  // 保存成功后关闭页签
  // value是一个数字，第一参数是当前路径，第二参数是目标路径
  closePath: function closePath(value) {
    var tageData = JSON.parse(sessionStorage.getItem('tageData'));
    var index = undefined;
    tageData.map(function (item, i) {
      var e = item.path.split('?')[0];
      if (value[0] === e) {
        index = i;
      }
    });
    var data = {
      index: index, // 当前要关闭的页面的下标
      jumpPath: value[1] // 要调到页面的目标路径
    };
    __WEBPACK_IMPORTED_MODULE_1__components_layout_components_bus__["a" /* default */].$emit('tagsIndex', data);
  }
});

