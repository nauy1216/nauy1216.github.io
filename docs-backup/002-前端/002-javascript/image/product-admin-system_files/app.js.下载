/***/ "./src/store/plugins.js":
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "a", function() { return vuexLocalStorage; });
/* harmony import */ var __WEBPACK_IMPORTED_MODULE_0__utils_auth__ = __webpack_require__("./src/utils/auth.js");

var vuexLocalStorage = function vuexLocalStorage(store) {
  // 当 store 初始化后调用
  var token = Object(__WEBPACK_IMPORTED_MODULE_0__utils_auth__["b" /* getToken */])();
  var user = Object(__WEBPACK_IMPORTED_MODULE_0__utils_auth__["c" /* getUser */])();
  if (token && user) {
    store.commit('LOGIN', {
      token: token,
      user: user
    });
  }
  store.subscribe(function (mutation, state) {
    if (mutation.type === 'LOGIN') {
      Object(__WEBPACK_IMPORTED_MODULE_0__utils_auth__["d" /* setToken */])(mutation.payload.token);
      Object(__WEBPACK_IMPORTED_MODULE_0__utils_auth__["e" /* setUser */])(mutation.payload.user);
    } else if (mutation.type === 'LOGOUT') {
      Object(__WEBPACK_IMPORTED_MODULE_0__utils_auth__["a" /* clear */])();
    }
  });
};

