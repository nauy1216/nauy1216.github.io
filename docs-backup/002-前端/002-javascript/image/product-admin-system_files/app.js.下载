// 使用钩子函数对路由进行权限跳转
__WEBPACK_IMPORTED_MODULE_2__router__["a" /* default */].beforeEach(function (to, from, next) {
  var data = [to, from];
  var flag = sessionStorage.getItem('flag');
  if (to.path !== '/login') {
    // 需要登录权限进入的路由
    if (!flag) {
      // 获取不到登录信息
      next({
        path: '/login'
      });
    } else {
      // 获取到登录信息，进行下一步
      nextRouter(data, next);
    }
  } else {
    // 不需要登录权限的路由直接进行下一步
    nextRouter(data, next);
  }
});
// 頁面中有表单被修改过，离开页面提示判断
function nextRouter(data, next) {
  var canLeavePage = __WEBPACK_IMPORTED_MODULE_3__store__["a" /* default */].state.canClosePageTag;
  if (!canLeavePage) {
    canLeavePageFn().then(function (res) {
      if (res) {
        if (localStorage.getItem('postSelectionItem')) {
          localStorage.removeItem('postSelectionItem');
        }
        if (localStorage.getItem('recievePostSelectionItem')) {
          localStorage.removeItem('recievePostSelectionItem');
        }
        next();
      }
    });
  } else {

    var tagsList = !!JSON.parse(sessionStorage.getItem('tageData')) ? JSON.parse(sessionStorage.getItem('tageData')) : [];
    if (tagsList.length > 29) {
      // 判断是否超过30个标签
      var result = tagsList.some(function (e) {
        // 判断打开标签是否在当前标签中  是返回true  不是返回  false
        // console.log('判断是否相等：',e.path.split('?')[0],data[0].path)
        if (e.path.split('?')[0] == data[0].path) {
          return true;
        }
      });
      if (!result) {
        // 如果是新标签  就弹出选择框
        return new __WEBPACK_IMPORTED_MODULE_0_babel_runtime_core_js_promise___default.a(function (resolve, reject) {
          __WEBPACK_IMPORTED_MODULE_4_element_ui__["MessageBox"].confirm('您已打开太多页签,将会关闭第一个页签，请确认是否关闭！', '业务提示', {
            distinguishCancelAndClose: true,
            confirmButtonText: '是',
            cancelButtonText: '否',
            type: 'warning'
          }).then(function () {
            next();
          }).catch(function () {
            next({ path: data[1].path });
          });
        });
      } else {
        next();
      }
    } else {
      next();
    }
  }
}
// 頁面中有表单被修改过，离开页面提示判断
function canLeavePageFn() {
  return new __WEBPACK_IMPORTED_MODULE_0_babel_runtime_core_js_promise___default.a(function (resolve, reject) {
    __WEBPACK_IMPORTED_MODULE_4_element_ui__["MessageBox"].confirm('您有未保存的数据,请确认是否离开！', '确认信息', {
      distinguishCancelAndClose: true,
      confirmButtonText: '是',
      cancelButtonText: '否',
      type: 'warning'
    }).then(function () {
      resolve(true);
      __WEBPACK_IMPORTED_MODULE_3__store__["a" /* default */].commit('setCanClosePageTag', true);
    }).catch(function () {
      resolve(false);
      __WEBPACK_IMPORTED_MODULE_3__store__["a" /* default */].state.canClosePageWatch();
      // 点击取消,取消掉watch
      __WEBPACK_IMPORTED_MODULE_3__store__["a" /* default */].commit('setCanClosePageTag', false);
    });
  });
}
__WEBPACK_IMPORTED_MODULE_2__router__["a" /* default */].onError(function (error) {
  console.log('当前项目有更新-------');
  var pattern = /Loading chunk (\d)+ failed/g;
  var isChunkLoadFailed = error.message.match(pattern);
  var targetPath = __WEBPACK_IMPORTED_MODULE_2__router__["a" /* default */].history.pending.fullPath;
  if (isChunkLoadFailed) {
    __WEBPACK_IMPORTED_MODULE_2__router__["a" /* default */].replace(targetPath);
  }
});
