/* harmony default export */ __webpack_exports__["a"] = (function (type, url, params, contentType, isresponse, isError) {
  // 拼凑出erp 需要的四个header内容
  var timestamp = Math.round(new Date().getTime() / 1000);
  var guid = function guid() {
    return 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function (c) {
      var r, v;
      r = Math.random() * 16 | 0;
      v = c === 'x' ? r : r & 0x3 | 0x8;
      return v.toString(16);
    });
  };

  var serialize = function serialize(obj) {
    var str = '';
    if (obj === null || obj === undefined) {
      return str;
    }
    __WEBPACK_IMPORTED_MODULE_1_babel_runtime_core_js_object_keys___default()(obj).forEach(function (key) {
      str += key + '=' + obj[key] + '&';
    });
    return str.trimRight('&');
  };

  __WEBPACK_IMPORTED_MODULE_2_axios___default.a.defaults.headers.post['contentType'] = 'application/x-www-form-urlencoded';
  __WEBPACK_IMPORTED_MODULE_2_axios___default.a.defaults.headers.common['timestamp'] = timestamp;
  __WEBPACK_IMPORTED_MODULE_2_axios___default.a.defaults.headers.common['ua'] = '5';
  // 客户端来源 先随便填一个 cookieGet('ua')
  __WEBPACK_IMPORTED_MODULE_2_axios___default.a.defaults.headers.common['source'] = '8';
  __WEBPACK_IMPORTED_MODULE_2_axios___default.a.defaults.headers.common['sn'] = guid();

  if (sessionStorage.getItem('authentication')) {
    // axios.defaults.headers.common['Authorization']
    // = 'bearer' + ' ' + sessionStorage.getItem('token')
    __WEBPACK_IMPORTED_MODULE_2_axios___default.a.defaults.headers.common['Authentication'] = JSON.parse(sessionStorage.getItem('authentication'));
  }
  return new __WEBPACK_IMPORTED_MODULE_0_babel_runtime_core_js_promise___default.a(function (resolve, reject) {
    switch (type) {
      case 'post':
        params = serialize(params);
        __WEBPACK_IMPORTED_MODULE_2_axios___default.a.post(url, params).then(function (response) {
          if (!isresponse) {
            resolve(response);
            // 需要返回全部 不然获取不到头部的分页数据类似情况
          } else {
            resolve(response);
          }
          if (response.data.code === 400) {
            //错误提示
            __WEBPACK_IMPORTED_MODULE_4_element_ui__["Message"].error(response.data.error);
          }
        }, function (err) {
          // 如果报401，则返回到登陆页面
          if (err.response.status === 401) {
            __WEBPACK_IMPORTED_MODULE_3__components_layout_components_bus__["a" /* default */].$emit('getToken');
          }

          if (!isError) {
            __WEBPACK_IMPORTED_MODULE_4_element_ui__["Message"].error(err.response.data);
          }
          console.log('错误日志', err.response);
          reject(err.response);
        }).catch(function (error) {
          reject(error);
          console.log('post--', error);
        });
        break;
      case 'postJson':
        __WEBPACK_IMPORTED_MODULE_2_axios___default.a.post(url, params).then(function (response) {
          if (!isresponse) {
            resolve(response);
            // 需要返回全部 不然获取不到头部的分页数据类似情况
          } else {
            resolve(response);
          }
          if (response.data.code === 400) {
            //错误提示
            __WEBPACK_IMPORTED_MODULE_4_element_ui__["Message"].error(response.data.error);
          }
        }, function (err) {
          // 如果报401，则返回到登陆页面
          if (err.response.status === 401) {
            __WEBPACK_IMPORTED_MODULE_3__components_layout_components_bus__["a" /* default */].$emit('getToken');
          }

          if (!isError) {
            __WEBPACK_IMPORTED_MODULE_4_element_ui__["Message"].error(err.response.data);
          }
          console.log('错误日志', err.response);
          reject(err.response);
        }).catch(function (error) {
          reject(error);
          console.log('post--', error);
        });
        break;
      case 'download':
        __WEBPACK_IMPORTED_MODULE_2_axios___default.a.post(url, params, { responseType: 'blob' }, { timeout: 3000 }).then(function (response) {
          if (response.status == 200) {
            resolve(response);
          } else {
            reject(response);
          }
        }, function (err) {
          // 如果报401，则返回到登陆页面
          if (err.response.status === 401) {
            __WEBPACK_IMPORTED_MODULE_3__components_layout_components_bus__["a" /* default */].$emit('getToken');
          }
          console.log('错误日志', err.response);
          // Message.error(err.response.data.message);
          reject(err.response);
        }).catch(function (error) {
          reject(error);
          console.log("post--", error);
        });
        break;
      case 'downloadGet':
        __WEBPACK_IMPORTED_MODULE_2_axios___default.a.get(url, params, { responseType: 'blob' }, { timeout: 3000 }).then(function (response) {
