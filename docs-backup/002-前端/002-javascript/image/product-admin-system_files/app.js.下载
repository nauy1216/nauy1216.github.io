/**************************路由权限拦截**********************************/
var sessionData = JSON.parse(sessionStorage.getItem('authorityData'));
// 获取有效路由链接
var getActiveLink = function getActiveLink(sessionData) {
  sessionData.map(function (item) {
    // console.log(item,'====item')
    if (item.link) {
      authLinks.push(item.link);
      if (item.children && item.children.length) {
        getActiveLink(item.children);
      }
    } else {
      // console.log(item.label,item,'++++item1')
      if (item.children && item.children.length) {
        getActiveLink(item.children);
      }
    }
  });
};
if (sessionData && sessionData.length) {
  getActiveLink(sessionData);
  // console.log(authLinks,'===authLinks')
  // 实施路由拦截
  console.log(authLinks);
  router.beforeEach(function (to, form, next) {
    var path = to.path.split('/')[1];
    if (path == 'login' || authLinks.includes(path)) {
      next();
    } else {
      next();
      __WEBPACK_IMPORTED_MODULE_5_element_ui__["Message"].error('您没有权限访问此链接');
    }
  });
}
/* harmony default export */ __webpack_exports__["a"] = (router);

/***/ }),

/***/ "./src/store/actions.js":
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
/* harmony import */ var __WEBPACK_IMPORTED_MODULE_0__utils_ajax__ = __webpack_require__("./src/utils/ajax.js");
/* harmony import */ var __WEBPACK_IMPORTED_MODULE_1__api__ = __webpack_require__("./src/api/index.js");
// import { login } from '@/api'
// import router from '../router'


var actions = {
  HANDLE_LOGIN: function HANDLE_LOGIN(context, payload) {
    // login(payload).then((result) => {
    //   const user = result
    //   context.commit('LOGIN', user)
    //   router.push({
    //     path: '/archivesProduct'
    //   })
    // })
  },
  getSalespersonList: function getSalespersonList(state, id) {
    Object(__WEBPACK_IMPORTED_MODULE_0__utils_ajax__["a" /* default */])('get', __WEBPACK_IMPORTED_MODULE_1__api__["a" /* default */].getDropDownSalesperson + ('/' + id)).then(function (res) {
      if (res.data.code === 200) {
        var results = res.data.data;
        results.forEach(function (e) {
          e.value = e.salespersonName;
          e.name = e.salespersonName;
          e.code = e.salespersonCode;
        });
        setTimeout(function () {
          state.commit('setInputSalespersonList', results);
        }, 50);
      }
    });
  }
};

/* harmony default export */ __webpack_exports__["a"] = (actions);

/***/ }),

/***/ "./src/store/getters.js":
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
var getters = {
  userGetter: function userGetter(state) {
    return state.user.name;
  }
};

/* harmony default export */ __webpack_exports__["a"] = (getters);

/***/ }),

/***/ "./src/store/index.js":
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
/* harmony import */ var __WEBPACK_IMPORTED_MODULE_0_vue__ = __webpack_require__("./node_modules/vue/dist/vue.runtime.esm.js");
/* harmony import */ var __WEBPACK_IMPORTED_MODULE_1_vuex__ = __webpack_require__("./node_modules/vuex/dist/vuex.esm.js");
